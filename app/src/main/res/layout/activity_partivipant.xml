<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Participant.PartivipantActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/Participant"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Welcome Participant ..."
            android:textColor="#080808"
            android:textSize="34sp" />


        <Button
            android:id="@+id/SearchEvents"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Finding new events"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            android:onClick="SearchEventsOnclick"/>

        <Button
            android:id="@+id/History"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Rate my events"
            android:textSize="25sp"
            android:layout_marginTop="15dp"
            android:onClick="ViewHistory"/>





    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>